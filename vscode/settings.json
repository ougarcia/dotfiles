{
  "editor.fontFamily": "'Berkeley Mono', 'JetBrains Mono', 'Symbols Nerd Font Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "storage.modifier",
          "storage.type",
          "support.type",
          "variable.parameter"
        ],
        "settings": { "fontStyle": "" }
      }
    ]
  },
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "terminal.integrated.fontFamily": "'Berkeley Mono', 'JetBrains Mono', 'Symbols Nerd Font Mono'",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Monokai Pro",
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.startupEditor": "none"
}
